source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '9.0'
use_frameworks!

abstract_target 'ZeusFramework' do
    pod 'Alamofire', :git => 'https://github.com/Alamofire/Alamofire.git', :commit => '3cc5b4e'
    pod 'SwiftyBeaver', :git => 'https://github.com/SwiftyBeaver/SwiftyBeaver.git', :branch => 'swift3'
    pod 'DateParser', :git => 'https://github.com/3lvis/DateParser', :branch => 'swift-3'
	target 'Zeus'
end

